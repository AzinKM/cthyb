# Multiple inclusion guard
if (NOT CTHYB_FOUND) 
set(CTHYB_FOUND True)

# version
set(CTHYB_VERSION @CTHYB_VERSION@)
set(CTHYB_GIT_HASH @CTHYB_GIT_HASH@)

# Root of the installation 
set(CTHYB_ROOT  @CMAKE_INSTALL_PREFIX@)

# include the exported targets of this project 
include(@CMAKE_INSTALL_PREFIX@/share/cmake/cthyb-targets.cmake)

MESSAGE(STATUS "Found CTHYBConfig.cmake with version ${CTHYB_VERSION}, hash = ${CTHYB_GIT_HASH}")

# Documentation
set(CTHYB_WITH_DOCUMENTATION @Build_Documentation@)

endif (NOT CTHYB_FOUND) 
